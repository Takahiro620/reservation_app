<h1>管理者向け商品一覧</h1>
<thead>
    <tr>
        <th>商品名</th>
        <th>商品詳細</th>
        <th>価格</th>
    </tr>
</thead>
<% @items.each do |item| %>
<tbody>
    <tr>
        <td><%= item.name %></td>
        <td><%= item.introduction %></td>
        <td><%= item.price %></td>
    </tr>
</tbody>
<%= link_to "詳細", admin_item_path(item) %>
<% end %>