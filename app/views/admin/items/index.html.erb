<h1>管理者向け商品一覧</h1>
<table>
    <thead>
        <tr>
            <th>商品名</th>
            <th>商品詳細</th>
            <th>価格</th>
            <th>詳細</th>
        </tr>
    </thead>
    <tbody>
    <% @items.each do |item| %>
    <tr>
        <td><%= item.name %></td>
        <td><%= item.introduction %></td>
        <td><%= item.price %></td>
        <td><%= link_to "詳細", admin_item_path(item) %></td>
    </tr>
    </tbody>
    <% end %>
</table>
