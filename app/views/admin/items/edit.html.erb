<h1>商品編集画面</h1>

<%= form_with model: @item, url: admin_item_path(@item)  do |f| %>
<h4>商品名</h4>
<%= f.text_field :name, autofocus: true, class: "form-control book_title"%>
<h4>商品詳細</h4>
<%= f.text_field :introduction %>
<h4>価格</h4>
<%= f.number_field :price %>
<%= f.submit "商品" , class: "btn btn-success"%>
<% end %>